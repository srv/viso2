<package>
  <description brief="ROS-Wrapper for libviso2">
    This is the ROS wrapper for libviso2, library for visual odometry (see package viso2).
  </description>
  <author>Stephan Wirth</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/viso2_ros</url>
  <versioncontrol type="git" url="git://github.com/srv/srv_vision.git"/>

  <depend package="viso2"/>
  <depend package="roscpp"/>
  <depend package="sensor_msgs"/>
  <depend package="nav_msgs"/>
  <depend package="message_filters"/>
  <depend package="image_transport"/>
  <depend package="cv_bridge"/>
  <depend package="image_geometry"/>
  <depend package="tf"/>

</package>


